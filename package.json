{
  "name": "store_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "st": "tsc && nodemon dist/app.js",
    "start": "ts-node src/*.ts lib/*.ts",
    "dev": "nodemon",
    "lint": "eslint ./ --ext ts",
    "prettier": "prettier --write '*.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0"
  }
}
